@model List<Kategori>
<div class="container mt-5">
    <form method="post" asp-action="FotografEkle" enctype="multipart/form-data">

        <div class="form-row">
            <div class="col">
                <input type="text" class="form-control" placeholder="Fotoğraf Açıklaması" name="FotografAciklamasi">
            </div>
        </div>
       
        <div class="form-group">

            <input type="file" name="file" id="file" class="form-control" />
        </div>

        <button type="submit" class="btn btn-primary">Yükle</button>
    </form>
</div>